{
  "version": 2,
  "builds": [
    {
      "src": "websocket-client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "websocket-client/build"
      }
    },
    {
      "src": "websocket-server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "websocket-server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "websocket-client/build/$1"
    }
  ]
}
